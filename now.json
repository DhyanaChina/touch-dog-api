{
  "name": "touch-dog",
  "version": 2,
  "alias": "touch-dog",
  "regions": ["sin1"],
  "github": {
    "enabled": false
  },
  "env": {
    "AWS_ACCESS_KEY_ID2": "@aws-access-key-id",
    "AWS_SECRET_ACCESS_KEY2": "@aws-secret-access-key"
  },
  "builds": [
    { "src": "*.js", "use": "now-micro" }
  ],
  "routes": [
    { "src": "/translate", "dest": "/index.js" }
  ]
}
