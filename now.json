{
  "name": "touch-dog",
  "version": 2,
  "alias": "touch-dog",
  "github": {
    "silent": true
  },
  "regions": ["hnd1"],
  "env": {
    "TRANSLATOR_TEXT_KEY": "@translator-azure-key"
  },
  "builds": [
    { "src": "*.js", "use": "now-micro" }
  ],
  "routes": [
    { "src": "/translate", "dest": "/index.js" },
    { "src": "/google", "dest": "/google.js" }
  ]
}
